  "sdf": [
    {
      "path": "/v3/async/make-photo",
      "method": "POST",
      "summary": "制作照片",
      "description": "通过 Stacked ID Embedding 定制逼真的人物照片，对于异步图像生成，只会返回一个 task_id。您应该使用 task_id 调用 /v3/async-batch/task-result API 端点以检索图像生成结果。您可以在此处获取指导：https://novita.ai/get-started/UseCase_ImageEnhancement.html#_18-make-photo。\n\n",
      "parameters": [
        {
          "name": "Authorization",
          "in": "header",
          "description": "",
          "required": true,
          "type": "string"
        },
        {
          "name": "Content-Type",
          "in": "header",
          "description": "",
          "required": true,
          "type": "string"
        }
      ],
      "requestBody": {
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "extra": {
                  "type": "object",
                  "properties": {
                    "response_image_type": {
                      "type": "string",
                      "description": "图像类型"
                    }
                  },
                  "required": [
                    "response_image_type"
                  ],
                  "description": "额外信息。"
                },
                "model_name": {
                  "type": "string",
                  "description": "基于 SDXL 的模型的名称，您可以使用参数 filter.types=checkpoint 调用 /v3/model 端点来检索 sd_name_in_api 字段作为 model_name。"
                },
                "image_assets_ids": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  },
                  "description": "图像的资产 ID"
                },
                "prompt": {
                  "type": "string",
                  "description": "肯定提示词，用“，”分隔。"
                },
                "negative_prompt": {
                  "type": "string",
                  "description": "否定提示词，用“,”分隔"
                },
                "loras": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "model_name": {
                        "type": "string",
                        "description": "基于SDXL的lora的名称，您可以调用/v3/models端点来检索sd_name字段作为model_name，记住不要包含“.safetensors”"
                      },
                      "strength": {
                        "type": "number",
                        "description": "洛拉的力量值。值越大，效果越偏向lora。"
                      }
                    }
                  },
                  "description": "Lora选项，目前最多支持5个Lora"
                },
                "strength": {
                  "type": "number",
                  "description": "它添加的噪声量是受控制的，值越大，与原始图像的差异越大。"
                },
                "width": {
                  "type": "integer",
                  "description": "图像的宽度。"
                },
                "height": {
                  "type": "integer",
                  "description": "图像的高度。"
                },
                "image_num": {
                  "type": "integer",
                  "description": "一代人生成的图像数量。"
                },
                "steps": {
                  "type": "integer",
                  "description": "将步骤视为图像创建过程的迭代。"
                },
                "seed": {
                  "type": "integer",
                  "description": "种子是稳定扩散产生噪声的数字。"
                },
                "guidance_scale": {
                  "type": "number",
                  "description": "此设置表示稳定扩散将听取您的提示的程度。"
                },
                "sampler_name": {
                  "type": "string",
                  "description": "这个去噪过程称为采样，因为稳定扩散在每个步骤中都会生成一个新的样本图像。"
                }
              },
              "required": [
                "extra",
                "model_name",
                "image_assets_ids",
                "prompt",
                "negative_prompt",
                "loras",
                "strength",
                "width",
                "height",
                "image_num",
                "steps",
                "seed",
                "guidance_scale",
                "sampler_name"
              ]
            }
          }
        }
      },
      "responses": {
        "200": {
          "description": "",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "task_id": {
                    "type": "string"
                  }
                },
                "required": [
                  "task_id"
                ]
              }
            }
          }
        }
      },
      "tags": [
        "案例 API"
      ],
      "directory": "sdf"
    },
